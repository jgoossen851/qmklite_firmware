
class XAPRouteError(Exception):
    pass


class XAPRoutes():
{%- for id, route in xap.routes | dictsort %}
{%- if route.routes %}
    # {{route.define}}
{%- for subid, subroute in route.routes | dictsort %}
    {{route.define}}_{{subroute.define}} = b'\x{{ '%02d' % id|int(base=16) }}\x{{ '%02d' % subid|int(base=16) }}'
{%- endfor %}
{%- endif %}
{%- endfor %}